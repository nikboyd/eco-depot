Object subclass: Unit.
"A unit of measure."

Unit class members: [

    @Protected! Hoot Magnitudes Integer! StandardPrecision := 4.
]

Unit members:
[
    @Primitive! String! printString [ ^String from: self getClass getSimpleName toLowerCase + 's' ]
]
